{
  "name": "openmined-website",
  "homepage": "https://openmined.org",
  "version": "0.1.0",
  "private": false,
  "license": "Apache-2.0",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "forcedomain": "^0.8.1",
    "husky": "^0.14.3",
    "flow-bin": "^0.57.3",
    "forcedomain": "^0.8.1",
    "ignore-styles": "^5.0.1",
    "lint-staged": "^4.1.3",
    "moment": "^2.19.3",
    "morgan": "^1.9.0",
    "node-sass-chokidar": "^0.0.3",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.1",
    "openmined-ui": "^0.0.5",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-render-html": "^0.6.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-scripts": "1.0.14",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": ["prettier --single-quote --write", "git add"]
  },
  "scripts": {
    "precommit": "lint-staged",
    "build:css":
      "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch:css":
      "npm run build:css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start:js": "react-scripts start",
    "dev:client": "npm-run-all -p watch:css start:js",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "npm run lint:js",
    "lint:js": "eslint .",
    "dev:server": "NODE_ENV=development node server/index.js",
    "start": "NODE_ENV=production node server/index.js"
  },
  "devDependencies": {
    "eslint": "^4.10.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "prettier": "^1.7.4"
  }
}
